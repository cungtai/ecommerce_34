<%- if controller_name != Settings.devise.sessions %>
  <%= link_to t("pages.sign_in"), new_session_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.registerable? && controller_name != Settings.devise.registrations %>
  <%= link_to t("pages.sign_up"), new_registration_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != Settings.devise.passwords && controller_name != Settings.devise.registrations %>
  <%= link_to t("users.forgot_password_title"), new_password_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != Settings.devise.confirmations %>
  <%= link_to t("users.not_recieve_confirm_link"), new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != Settings.devise.unlocks %>
  <%= link_to t("users.not_recieve_unlock_link"), new_unlock_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to t("users.sign_in_with") + "#{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %><br />
  <% end -%>
<% end -%>
